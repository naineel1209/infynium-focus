{
    "manifest_version": 3,
    "name": "Unlimited Block Sites",
    "version": "1.0",
    "description": "A Chrome extension to bypass blocked websites.",
    "permissions": [
        "storage",
        "tabs",
        "browsingData",
        "activeTabs",
        "webRequest",
        "webRequestBlocking",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}